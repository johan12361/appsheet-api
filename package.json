{
  "name": "appsheet-api",
  "version": "0.3.9",
  "type": "module",
  "description": "A TypeScript client for the AppSheet API",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsc --watch",
    "playground": "tsx playground/playground.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "pnpm typecheck && pnpm lint && pnpm test:run && pnpm build"
  },
  "devDependencies": {
    "@types/luxon": "^3.7.1",
    "@types/node": "24.10.1",
    "@typescript-eslint/eslint-plugin": "8.48.0",
    "@typescript-eslint/parser": "8.48.0",
    "@vitest/ui": "^4.0.15",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "prettier": "3.7.3",
    "tsup": "8.5.1",
    "tsx": "4.20.6",
    "typescript": "5.9.3",
    "vitest": "^4.0.15"
  },
  "keywords": [
    "appsheet",
    "api",
    "client",
    "typescript",
    "google"
  ],
  "author": "johan12361",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johan12361/appsheet-api.git"
  },
  "bugs": {
    "url": "https://github.com/johan12361/appsheet-api/issues"
  },
  "homepage": "https://github.com/johan12361/appsheet-api#readme",
  "packageManager": "pnpm@10.25.0",
  "dependencies": {
    "axios": "1.13.2",
    "luxon": "3.7.2"
  }
}
